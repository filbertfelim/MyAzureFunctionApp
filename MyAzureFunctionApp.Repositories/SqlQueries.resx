  <data name="GetAllAuthors" xml:space="preserve">
    <value>        SELECT A."AuthorId", A."Name", B."BookId", B."Title", B."ImagePath"       FROM "Authors" A        LEFT JOIN "Books" B ON A."AuthorId" = B."AuthorId";      </value>
    <comment/>
  </data>
  <data name="GetAuthorById" xml:space="preserve">
    <value>        SELECT A."AuthorId", A."Name", B."BookId", B."Title", B."ImagePath"        FROM "Authors" A        LEFT JOIN "Books" B ON A."AuthorId" = B."AuthorId"        WHERE A."AuthorId" = @Id;      </value>
    <comment/>
  </data>
  <data name="AddAuthor" xml:space="preserve">
    <value>INSERT INTO "Authors" ("Name") VALUES (@Name) RETURNING *;</value>
    <comment/>
  </data>
  <data name="UpdateAuthor" xml:space="preserve">
    <value>UPDATE "Authors" SET "Name" = @Name WHERE "AuthorId" = @AuthorId RETURNING *;</value>
    <comment/>
  </data>
  <data name="DeleteAuthor" xml:space="preserve">
    <value>DELETE FROM "Authors" WHERE "AuthorId" = @Id;</value>
    <comment/>
  </data>
  <data name="GetAllBooks" xml:space="preserve">
    <value>
    SELECT B."BookId", B."Title", B."AuthorId", B."ImagePath",
       A."AuthorId", A."Name",
       BC."BookId", BC."CategoryId",
       C."CategoryId", C."Name"
FROM "Books" B
LEFT JOIN "Authors" A ON B."AuthorId" = A."AuthorId"
LEFT JOIN "BookCategories" BC ON B."BookId" = BC."BookId"
LEFT JOIN "Categories" C ON BC."CategoryId" = C."CategoryId";
    </value>
    <comment/>
  </data>
  <data name="GetBookById" xml:space="preserve">
    <value>
    SELECT B."BookId", B."Title", B."AuthorId", B."ImagePath",
       A."AuthorId", A."Name",
       BC."BookId", BC."CategoryId",
       C."CategoryId", C."Name"
    FROM "Books" B
    LEFT JOIN "Authors" A ON B."AuthorId" = A."AuthorId"
    LEFT JOIN "BookCategories" BC ON B."BookId" = BC."BookId"
    LEFT JOIN "Categories" C ON BC."CategoryId" = C."CategoryId"
    WHERE B."BookId" = @Id;
    </value>
    <comment/>
  </data>
  <data name="AddBook" xml:space="preserve">
    <value>
    INSERT INTO "Books" ("Title", "AuthorId", "ImagePath") 
    VALUES (@Title, @AuthorId, '')
    RETURNING "BookId", "Title", "AuthorId", "ImagePath";
    </value>
    <comment/>
  </data>
  <data name="UpdateBook" xml:space="preserve">
    <value>
    UPDATE "Books" 
    SET "Title" = @Title, "AuthorId" = @AuthorId 
    WHERE "BookId" = @BookId 
    RETURNING "BookId", "Title", "AuthorId";
    </value>
    <comment/>
  </data>
  <data name="DeleteBook" xml:space="preserve">
