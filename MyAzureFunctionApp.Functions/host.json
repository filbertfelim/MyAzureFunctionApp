{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Trace",
      "Function": "Trace",
      "Microsoft": "Warning",
      "Worker": "Trace"
    }
  },
  "extensions": {
    "OpenApi": {
      "Version": "v2",
      "Info": {
        "Title": "My Azure Function API",
        "Description": "API documentation for My Azure Function App",
        "Version": "1.0.0"
      }
    }
  }
}
