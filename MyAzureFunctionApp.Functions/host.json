{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Trace",
      "Function": "Trace",
      "Microsoft": "Warning",
      "Worker": "Trace"
    }
  }
}
